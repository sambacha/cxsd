{

  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "experimentalDecorators": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "outDir": "./dist",
    "removeComments": false,
    "lib": [
      "dom",
      "es2015"
    ],
    "target": "es5"
  },
  "include": [
    "src/**/*.ts"
  ]
}

